    <script type="text/javascript" src="~/Tema/assets/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/Tema/assets/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Tema/assets/js/scripts.js"></script>
    <!-- Isotope - Portfolio Sorting -->
    <script type="text/javascript" src="~/Tema/assets/js/jquery.isotope.js"></script>
    <!-- Mobile Menu - Slicknav -->
    <script type="text/javascript" src="~/Tema/assets/js/jquery.slicknav.js"></script>
