                                    <li>
                                        <a href="~/Restoran/Index/">Kafe</a>
                                    </li>
                                    <li>
                                        <a href="~/DugunSalon/Index/">Düğün Salonu</a>
                                    </li>
                                    <li>
                                        <span>Etkinlik</span>
                                        <ul>
                                            <li>
                                                <a href="~/KafeBlog/Blog/">Kafe</a>
                                            </li>
                                            <li>
                                                <a href="~/DugunSalonBlog/Blog/">Restoran</a>
                                            </li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="~/Restoran/Hakkimizda/">Hakkımızda</a>
                                    </li>
                                    <li>
                                        <a href="~/Restoran/iletisim/">İletişim</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                    <!-- End Top Menu -->
                </div>
               @RenderBody()
                <div id="base" class="container padding-vert-30">
                    @Html.Action("AdresLaut","Restoran")
                </div>
                <!-- Footer Menu -->
                
                <!-- End Footer Menu -->
